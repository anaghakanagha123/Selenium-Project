<suite name="crossBrowserExecuting" thread-count="5" parallel="tests">
	<listeners>
		<listener
			class-name="com.sevenrmartsupermarket.listeners.ExtentReporter"></listener>
	</listeners>

	<test name="executeTestInChrome">
	<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="com.sevenrmartsupermarket.tests.LoginTest">
			</class>
		</classes>
	</test>

	<test name="executeTestInEdge">
	<parameter name="browser" value="edge"></parameter>
		<classes>
			<class name="com.sevenrmartsupermarket.tests.LoginTest">
			</class>
		</classes>
	</test>
	
		<test name="executeTestInFirefox">
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="com.sevenrmartsupermarket.tests.LoginTest">
			</class>
		</classes>
	</test>

</suite> 